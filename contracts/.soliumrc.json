{
    "extends": "solium:recommended",
    "plugins": [
      "security"
    ],
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "indentation": [
        "error",
        4
      ],
      "security/no-inline-assembly": 0,
      "security/no-block-members": 0,
      "error-reason": 0
    }
  }